{
	"name": "201911-komaba-fes-auto-deployment",
	"displayName": "201911-komaba-fes-auto-deployment",
	"description": "",
	"publisher": "chelproc",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "utcode.workshop.komaba-fes2019.deploy",
				"title": "Webに公開"
			},
			{
				"command": "utcode.workshop.komaba-fes2019.template",
				"title": "テンプレート"
			}
		],
		"configuration": {
			"title": "ut.code();",
			"properties": {
				"utcode.googleChromeExcutablePath": {
					"type": "string",
					"description": "Google Chrome Excutable Path",
					"default": "C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe"
				},
				"utcode.templatePath": {
					"type": "string"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/extract-zip": "^1.6.2",
		"@types/fs-extra": "^8.0.1",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/puppeteer-core": "^2.0.0",
		"@types/rimraf": "^2.0.3",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"extract-zip": "^1.6.7",
		"fs-extra": "^8.1.0",
		"open": "^7.0.0",
		"puppeteer-core": "^2.0.0",
		"zip-a-folder": "0.0.9"
	}
}
